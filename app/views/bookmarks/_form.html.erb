<%= form_for @bookmark do |form| %>
  <div class="row">
    <%= form.label :title %>
    <div class="col-md-6">
      <%= form.text_field :title %>
    </div> 
  </div>

  <div class="row">
    <%= form.label :url %>
    <div class="col-md-6">
      <%= form.text_field :url %>
    </div>
  </div>

  <div class="row">
    <%= form.label :shortening %>
    <div class="col-md-6">
      <%= form.text_field :shortening %>
    </div>
  </div>
  
  <%= form.submit %>
<% end %>
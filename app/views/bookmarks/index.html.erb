<h1>today is the Liberation Day!</h1>


<p>Viva!</p>

<h2>Create New Bookmark</h2>
<%= render partial: 'form' %>

<h2>All Bookmarks</h2>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Site</th>
    </tr>
  </thead>
  <tbody>
    <% @bookmarks.each do |bookmark| %>
    <tr>
        <td><%= link_to bookmark.title, bookmark %></td>
        <td><%= bookmark.site.top_level_url %></td>
    </tr>
    <% end %> 
  </tbody>
</table>
